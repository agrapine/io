{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
